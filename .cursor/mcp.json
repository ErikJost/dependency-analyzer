{
  "mcpServers": {
    "Web Research": {
      "command": "npx",
      "args": [
        "-y",
        "@mzxrai/mcp-webresearch@latest"
      ]
    },
    "gordon": {
      "command": "docker",
      "args": [
        "ai",
        "mcpserver"
      ]
    },
    "azure-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
