    },
    "dependencyAnalyzer": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-p",
        "8000:8000",
        "-v",
        "/Users/erikjost/data:/data",
        "-v",
        "/Users/erikjost:/Users/erikjost",
        "mcp/sdk-minimal"
      ]
    }
    }
  }