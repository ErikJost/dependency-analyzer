{
  "mcpServers": {
    "Browser Tools": {
      "command": "npx",
      "args": [
        "-y",
        "@agentdeskai/browser-tools-mcp@1.2.0"
      ]
    },
    "Web Research": {
      "command": "npx",
      "args": [
        "-y",
        "@mzxrai/mcp-webresearch@latest"
      ]
    },
    "github": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "ghcr.io/cursor-ai/github-mcp:latest"
      ],
      "env": {}
    },
    "gordon": {
      "command": "docker",
      "args": [
        "ai",
        "mcpserver"
      ]
    },
    "azure-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@Streen9/azure-mcp"
      ]
    },
    "dependencyAnalyzer": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-v",
        "mcp_dependency_data:/data",
        "mcp/sdk-minimal"
      ],
      "env": {}
    },
    "codeium": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "ghcr.io/cursor-ai/codeium-mcp:latest"
      ],
      "env": {}
    }
  }
}