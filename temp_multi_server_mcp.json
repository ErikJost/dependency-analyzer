{
  "mcpServers": {
    "github": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "ghcr.io/cursor-ai/github-mcp:latest"
      ],
      "env": {}
    },
    "dependencyAnalyzer": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-v",
        "mcp_dependency_data:/data",
        "mcp/sdk-minimal"
      ],
      "env": {}
    }
  }
} 